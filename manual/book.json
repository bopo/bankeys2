{
    "title": "大众版项目说明",
    "author": "BoPo",	
    "plugins": [
        "todo",
        "js-sequence-diagram-full@>=0.3.1",
        "toggle-chapters",
        "tbfed-pagefooter",
        "include-codeblock",
        "image-captions",
        "splitter",
        "new-flowchart"
